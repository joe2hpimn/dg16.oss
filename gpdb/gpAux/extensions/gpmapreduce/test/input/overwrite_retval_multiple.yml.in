%YAML 1.1
---
VERSION:   1.0.0.1
USER:      @db_user@

DEFINE:
  - INPUT:
      NAME:      data
      QUERY: "select * from simple"
      COLUMNS:  [m int, n int]

  - OUTPUT:
      NAME:     out_1
      FILE:     @abs_srcdir@/output/overwrite_retval_multiple.1
      DELIMITER: '|'

  - FINALIZE:
      NAME: retcomposite 
      PARAMETERS:
        - state int 
      RETURNS:
        - f1_ integer
        - f2_ integer
        - f3_ integer 

  - TRANSITION:
      NAME: tran 
      PARAMETERS:
        - m int 
        - n int
      RETURNS:
        - state int 
  
  - REDUCE:
      NAME: reducer 
      TRANSITION: tran 
      FINALIZE: retcomposite
   
  - TASK:
       NAME: worker
       SOURCE: data 
       REDUCE: reducer 

EXECUTE:
  - RUN:
      SOURCE:  worker
      TARGET:  out_1
