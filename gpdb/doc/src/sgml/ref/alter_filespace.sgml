 <refnamediv>
  <refname>ALTER FILESPACE</refname>
  <refpurpose>change the name or owner of a filespace</refpurpose>
 </refnamediv>

 <indexterm zone="sql-alterfilespace">
  <primary>ALTER FILESPACE</primary>
 </indexterm>

 <refsynopsisdiv>
<synopsis>

ALTER FILESPACE name RENAME TO newname

ALTER FILESPACE name OWNER TO newowner

</synopsis>
 </refsynopsisdiv>

